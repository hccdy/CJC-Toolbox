<Window x:Class="CJC_Advanced_Midi_Merger.SaveWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:mdfk="http://schemas.xceed.com/wpf/xaml/toolkit"
        xmlns:local="clr-namespace:CJC_Advanced_Midi_Merger"
        mc:Ignorable="d"
        Title="{DynamicResource SaveGroup}" Height="120" Width="200" ResizeMode="NoResize" Icon="../Resources/CJCAMMIcon.ico">
    <Grid>
        <DockPanel Height="24" LastChildFill="False" Margin="10,-45,0,0">
            <Label Content="{DynamicResource Setppq}" HorizontalAlignment="Left" VerticalAlignment="Top" DockPanel.Dock="Left"/>
            <mdfk:DecimalUpDown x:Name="ppq" Value="1920" Increment="1" Maximum="65535" Minimum="1" HorizontalAlignment="Left" Width="82" Height="24" VerticalAlignment="Top" DockPanel.Dock="Left" />
        </DockPanel>
        <Button Content="{DynamicResource Save}" Margin="10,0,10,10" Height="30" VerticalAlignment="Bottom" Click="okclick"/>
    </Grid>
</Window>
